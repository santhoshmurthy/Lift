<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff">

    <ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

       >


    <ImageView
        android:id="@+id/line1"
        android:layout_width="1000dp"
        android:layout_height="60dp"
        android:paddingBottom="15dp"
        android:paddingTop="30dp"
        android:src="@drawable/line_bar" />
    <com.LAW.Lift.model.MyTextviews
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="M/s LIFT INDIA"
        android:textSize="18sp"
        android:layout_marginLeft="5dp"
        android:textColor="#4c4c4c"
        android:layout_below="@+id/line1"
        android:id="@+id/textView2" />

    <com.LAW.Lift.model.MyTextviews
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C/O M/s.sai,Bharat,ilan"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="5dp"
        android:textSize="18sp"
        android:textColor="#4c4c4c"
        android:layout_below="@+id/textView2"
        android:id="@+id/textView3" />

    <com.LAW.Lift.model.MyTextviews
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Advocates"
        android:textSize="18sp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="3dp"
        android:textColor="#4c4c4c"
        android:layout_below="@+id/textView3"
        android:id="@+id/textView4" />

    <com.LAW.Lift.model.MyTextviews
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="II FLOOR"
        android:layout_marginTop="3dp"
        android:textSize="18sp"
        android:layout_marginLeft="5dp"
        android:textColor="#4c4c4c"
        android:layout_below="@+id/textView4"
        android:id="@+id/textView5" />

    <com.LAW.Lift.model.MyTextviews
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vanguard House"
        android:layout_marginTop="3dp"
        android:textSize="18sp"
        android:layout_marginLeft="5dp"
        android:textColor="#4c4c4c"
        android:layout_below="@+id/textView5"
        android:id="@+id/textView6" />

    <com.LAW.Lift.model.MyTextviews
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#4c4c4c"
        android:layout_marginLeft="5dp"
        android:text="148/48,Moore Street/2nd Lane Beach,"
        android:layout_marginTop="3dp"
        android:id="@+id/textView7"
        android:textSize="18sp"
        android:layout_below="@+id/textView6"/>

    <com.LAW.Lift.model.MyTextviews
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chennai-600001"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/textView8"
        android:textColor="#4c4c4c"
        android:textSize="18sp"
        android:layout_below="@+id/textView7"/>
        <com.LAW.Lift.model.MyTextviews
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="5dp"
            android:text="Email: lawinfingertips@gmail.com"
            android:id="@+id/textView9"
            android:textColor="#4c4c4c"
            android:layout_below="@+id/textView8"/>
        <ImageView
            android:id="@+id/line2"
            android:layout_width="1000dp"
            android:layout_height="60dp"
            android:paddingBottom="15dp"
            android:paddingTop="10dp"
            android:layout_below="@+id/textView9"
            android:src="@drawable/line_bar" />

        <com.LAW.Lift.model.MyTextview
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Contact Us"
            android:id="@+id/textView10"
            android:textSize="25dp"
            android:textColor="#000"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="10dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

    </RelativeLayout>
    </ScrollView>


</RelativeLayout>
